							"PubSubBookmarkPersistenceConfig": {
								"bookmarkPersistenceType": "VFS",
								"bookmarkNamespace": "",
								"CustomConfig": {
									"vfsRootPath": "InstallPath/bookmarks",
									"persistenceStructure": "clientType/{clientType}/topicName/{topicName}"
								}
							}								
						}
					}
				}
			}
		}
	},
	"Main": {
		"AppArgs": {
			"channelCount": 1,
			"globalStandbyEnabled": false,
			"pubSub2PubSubStartChannelID": 0,
			"restApiPort": 10000,
			"restBasicHealthUri": "/health/basic",
			"transmitMetrics": true,
			"outputDir": "$MODULEDIR$"
		}
	}
}