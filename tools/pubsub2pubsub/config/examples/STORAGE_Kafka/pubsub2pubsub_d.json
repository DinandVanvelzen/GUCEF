											"CustomConfig": {
												"desiredMinimalSerializedBlockSize": 5000000,
												"desiredMaxTimeToWaitToGrowSerializedBlockSizeInMs": 15000,
												"vfsStorageRootPath": "InstallPath",
												"vfsFileExtention": "pubsubcapture",
												"encodeCodecFamily": "",
												"encodeCodecName": "",
												"decodeCodecFamily": "",
												"decodeCodecName": "",
