											"needSubscribeSupport": true,
											"preferDedicatedConnection": false,
											"topicName": ".\\PRIVATE$\\test",
											"CustomConfig": {
												"convertMsmqClsIdToString": true,
												"convertMsmqMsgIdToString": true,
												"defaultMsmqBodyBufferSizeInBytes": 4096,
